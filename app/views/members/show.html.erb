<%= render 'navbar'%>
<div class="container">
     <h3><%= @member.name %></h3>
     <p><%= @member.age %></p>
      <small>
        <p>Secret Identity - <%= @member.secret_identity %></p>
      </small> 
      <small>
        <p>Powers - <%= @member.powers %></p>
      </small> 
</div>

<div class="container">
     <%= link_to("Edit", edit_member_path(@member.id), class:"btn btn-success")%>
     <%= link_to("Delete",member_path(@member),method: :delete,data:{confirm: "Are you sure you want to delete ?"}, class:"btn btn-danger") %>
</div>